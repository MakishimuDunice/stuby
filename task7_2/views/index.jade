extends layout

block content

  div#all_cont_wrapper
    h1= "Task 7"
    p Enter value of center and radius of doctor searching.
    div#lbl_cont
      div#top_cont_lbl
        label Enter point coordinates
          input#lat_inp(placeholder = 'latitude')

          input#long_inp(placeholder = 'longitude')
      div#bot_cont_ldl
        label Enter radius' value
          input#bot_inp(placeholder = 'radius')
  button#go_button
  div#map_cont

  div#addresses From below, you can see the doctors' addresses list.
      #list
        include list.jade